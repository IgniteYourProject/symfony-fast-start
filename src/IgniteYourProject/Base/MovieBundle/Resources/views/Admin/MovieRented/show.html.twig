{% extends '::Backend/layout.html.twig' %}

{% block content -%}
    <h1>MovieRented</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Movie_id</th>
                <td>{{ entity.movieid }}</td>
            </tr>
            <tr>
                <th>Renter_name</th>
                <td>{{ entity.rentername }}</td>
            </tr>
            <tr>
                <th>Renter_mobile</th>
                <td>{{ entity.rentermobile }}</td>
            </tr>
            <tr>
                <th>Rented_at</th>
                <td>{{ entity.rentedat|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Delivered_at</th>
                <td>{{ entity.deliveredat|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ entity.createdat|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Updated_at</th>
                <td>{{ entity.updatedat|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('admin_movie_rented') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('admin_movie_rented_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
